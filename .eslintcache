d){\r\n    return <Redirect to=\"/login\"/>\r\n  }\r\n const {role:{menus}} = store.getUser();\r\n  return (\r\n    <Layout style={{minHeight: '100%'}}>\r\n      <Sider>\r\n        <Leftnav></Leftnav>\r\n      </Sider>\r\n      <Layout>\r\n        <Header></Header>\r\n        <Content style={{margin: 20, backgroundColor: '#fff'}}>\r\n          <Switch>\r\n          {/* <Route path='/home' component={Home}/> */}\r\n              {/* {componentList.map((item)=>{\r\n                \r\n              })} */}\r\n              {/* <Redirect from='/' exact to='/home'/>\r\n              <Route path='/home' component={Home}/>\r\n              <Route path='/category' component={Category}/>\r\n              <Route path='/product' component={Product}/>\r\n              <Route path='/user' component={User}/>\r\n              <Route path='/role' component={Role}/>\r\n              <Route path=\"/charts/bar\" component={Bar}/>\r\n              <Route path=\"/charts/pie\" component={Pie}/>\r\n              <Route path=\"/charts/line\" component={Line}/>\r\n              <Route path=\"/order\" component={Order}/> */}\r\n          </Switch>\r\n        </Content>\r\n        <Footer style={{textAlign: 'center', color: '#cccccc'}}>推荐使用谷歌浏览器，可以获得更佳页面操作体验</Footer>\r\n      </Layout>\r\n    </Layout>\r\n  )\r\n}\r\nexport default Admin;\r\n","D:\\desk\\尚硅谷后台管理\\new后台管理模板\\admin-templete\\src\\utiils\\storageUtils.js",[],"D:\\desk\\尚硅谷后台管理\\new后台管理模板\\admin-templete\\src\\pages\\category\\category.js",[],"D:\\desk\\尚硅谷后台管理\\new后台管理模板\\admin-templete\\src\\pages\\user\\user.js",[],"D:\\desk\\尚硅谷后台管理\\new后台管理模板\\admin-templete\\src\\pages\\line\\line.js",[],"D:\\desk\\尚硅谷后台管理\\new后台管理模板\\admin-templete\\src\\pages\\product\\product.js",[],"D:\\desk\\尚硅谷后台管理\\new后台管理模板\\admin-templete\\src\\pages\\role\\role.js",[],"D:\\desk\\尚硅谷后台管理\\new后台管理模板\\admin-templete\\src\\pages\\pie\\pie.js",[],"D:\\desk\\尚硅谷后台管理\\new后台管理模板\\admin-templete\\src\\pages\\bar\\bar.js",[],"D:\\desk\\尚硅谷后台管理\\new后台管理模板\\