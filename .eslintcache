       if (items) {\r\n            this.openKey = item.key\r\n          }\r\n          return (\r\n            <SubMenu\r\n              key={item.key}\r\n              title={\r\n                <span>\r\n                  <Icon type={item.icon} />\r\n                  <span>{item.title}</span>\r\n                </span>\r\n              }\r\n            >\r\n              {this.renderMenu(item.children)}\r\n            </SubMenu>\r\n          )\r\n        }\r\n        return (\r\n          <Menu.Item key={item.key}>\r\n             <Link to={item.key}>\r\n              <Icon type={item.icon} />\r\n              <span>{item.title}</span>\r\n             </Link>\r\n          </Menu.Item>\r\n        )\r\n      }\r\n    })\r\n  }\r\n  render() {\r\n    // 得到当前请求的路由路径\r\n    let path = this.props.location.pathname;\r\n    if(path==='/'){\r\n      path='/home'\r\n    }\r\n    // 得到需要打开菜单项的key\r\n    const openKey = this.openKey\r\n    return (\r\n      <div className=\"left-nav\">\r\n        <Link to='/home' className=\"left-nav-header\">\r\n          <img src={Logo} alt=\"logo\" />\r\n          <h1>几维后台</h1>\r\n        </Link>\r\n        <Menu\r\n          defaultSelectedKeys={[path]}\r\n          defaultOpenKeys={[openKey]}\r\n          mode=\"inline\"\r\n          theme=\"dark\"\r\n        >\r\n          {this.menuNodes}\r\n        </Menu>\r\n      </div>\r\n    )\r\n  }\r\n}\r\nexport default withRouter(Leftnav);","D:\\desk\\尚硅谷后台管理\\new后台管理模板\\admin-templete\\src\\config\\mainComponent.js",[],"D:\\desk\\尚硅谷后台管理\\new后台管理模板\\admin-templete\\src\\pages\\home\\home.js",[],"D:\\desk\\尚硅谷后台管理\\new后台管理模板\\admin-templete\\src\\pages\\not-found\\not-found.jsx",[],{"ruleId":"83","replacedBy":"84"},{"ruleId":"85","replacedBy":"86"},{"ruleId":"87","severity":1,"message":"88","line":31,"column":32,"nodeType":"89","messageId":"90","endLine":31,"endColumn":34},"no-native-reassign",["91"],"no-negated-in-lhs",["92"],"array-callback-return","Array.prototype.map() expects a value to 